@article{lopesIonosphericScintillationMitigation2023,
  title = {Ionospheric {{Scintillation Mitigation With Kalman PLLs Employing Radial Basis Function Networks}}},
  author = {Lopes, Rafael A. M. and Antreich, Felix and Fohlmeister, Friederike and Kriegel, Martin and Kuga, Helio K.},
