}

@article{carranoTheoryScintillationTwocomponent2016,
  title = {A Theory of Scintillation for Two-component Power Law Irregularity Spectra: {{Overview}} and Numerical Results},
  shorttitle = {A Theory of Scintillation for Two-component Power Law Irregularity Spectra},
