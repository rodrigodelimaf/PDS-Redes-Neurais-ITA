@article{campsIonosphericEffectsGNSSReflectometry2016,
  title = {Ionospheric {{Effects}} in {{GNSS-Reflectometry From Space}}},
  author = {Camps, Adriano and Park, Hyuk and Foti, Giuseppe and Gommenginger, Christine},
  year = {2016},
  month = dec,
  journal = {IEEE Journal of Selected Topics in Applied Earth Observations and Remote Sensing},
  volume = {9},
