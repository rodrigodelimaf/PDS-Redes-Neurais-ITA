@inproceedings{vila-vallsAdvancedKFbasedMethods2015,
  title = {Advanced {{KF-based}} Methods for {{GNSS}} Carrier Tracking and Ionospheric Scintillation Mitigation},
  booktitle = {2015 {{IEEE Aerospace Conference}}},
  author = {{Vil{\`a}-Valls}, Jordi and Closas, Pau and {Fern{\`a}ndez-Prades}, Carles},
