@article{carranoSimulatingImpactsIonospheric2012,
  title = {Simulating the Impacts of Ionospheric Scintillation on {{L}} Band {{SAR}} Image Formation},
  author = {Carrano, Charles S. and Groves, Keith M. and Caton, Ronald G.},
