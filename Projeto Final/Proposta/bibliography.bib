@article{vaniNovelApproachImprove2019,
  title = {A {{Novel Approach}} to {{Improve GNSS Precise Point Positioning During Strong Ionospheric Scintillation}}: {{Theory}} and {{Demonstration}}},
  shorttitle = {A {{Novel Approach}} to {{Improve GNSS Precise Point Positioning During Strong Ionospheric Scintillation}}},
  author = {Vani, Bruno Cesar and Forte, Biagio and Monico, Joao Francisco Galera and Skone, Susan and Shimabukuro, Milton Hirokazu and Moraes, Alison de Oliveira and Portella, Igor Ponte and Marques, Haroldo Antonio},
  year = {2019},
